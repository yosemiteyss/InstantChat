<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    app:startDestination="@id/overviewFragment">

    <fragment
        android:id="@+id/overviewFragment"
        android:name="com.yosemitedev.instantchat.ui.overview.OverviewFragment"
        android:label="OverviewFragment"
        tools:layout="@layout/fragment_overview">
        <action
            android:id="@+id/action_overviewFragment_to_contactEditFragment"
            app:destination="@id/contactEditFragment" />
    </fragment>

    <fragment
        android:id="@+id/contactEditFragment"
        android:name="com.yosemitedev.instantchat.ui.overview.contact.ContactEditFragment"
        android:label="ContactEditFragment"
        tools:layout="@layout/fragment_contact_edit"/>

</navigation>